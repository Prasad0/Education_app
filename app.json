{
  "expo": {
    "name": "Coaching Finder",
    "slug": "coaching-finder-app",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "sdkVersion": "54.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prasad01.coachingfinderapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "./plugins/withNetworkSecurity"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f71f027e-506c-4209-a1f6-64ff74ab6905"
      }
    }
  }
}
